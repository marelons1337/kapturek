<div id="<%= dom_id client %>">
  <p>
    <strong>Name:</strong>
    <%= client.name %>
  </p>

  <p>
    <strong>Surname:</strong>
    <%= client.surname %>
  </p>

  <p>
    <strong>Phone:</strong>
    <%= client.phone %>
  </p>

  <p>
    <strong>Email:</strong>
    <%= client.email %>
  </p>

  <p>
    <strong>Rent from:</strong>
    <%= client.rent_from %>
  </p>

  <p>
    <strong>Rent to:</strong>
    <%= client.rent_to %>
  </p>

  <p>
    <strong>Paid:</strong>
    <%= client.paid %>
  </p>

  <p>
    <strong>Debt:</strong>
    <%= client.debt %>
  </p>

  <p>
    <strong>Account:</strong>
    <%= client.account_id %>
  </p>

  <p>
    <strong>Rental:</strong>
    <%= client.rental_id %>
  </p>

  <p>
    <strong>Sale:</strong>
    <%= client.sale_id %>
  </p>

  <p>
    <strong>Company:</strong>
    <%= client.company %>
  </p>

  <p>
    <strong>Note:</strong>
    <%= client.note %>
  </p>

</div>
